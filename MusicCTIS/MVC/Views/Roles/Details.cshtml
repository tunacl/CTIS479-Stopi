@model RoleModel
@*Generated from Custom Template.*@
@*Model namespace using directive should be added to _ViewImports.cshtml.*@

@{
    ViewData["Title"] = "Role Details";
}

<h1>@ViewData["Title"]</h1>

<div>
    <h4>Role</h4>
    <hr />
        <dl class="row">
            <dt class="col-2">
                @Html.DisplayNameFor(model => model.Name)
            </dt>
            <dd class="col-10">
                @Html.DisplayFor(model => model.Name)
            </dd>
        </dl>
        <dl class="row">
            <dt class="col-2">
                @Html.DisplayNameFor(model => model.UserCount)
            </dt>
            <dd class="col-10">
                @Html.DisplayFor(model => model.UserCount)
            </dd>
        </dl>
    @if (Model.UserCount > 0)
    {
        <dl class="row">
            <dt class="col-2">
                @Html.DisplayNameFor(model => model.Users)
            </dt>
            <dd class="col-10">
                @Html.Raw(Model.Users)
            </dd>
        </dl>
    }

</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Edit</a>&nbsp;|&nbsp;
    <a asp-action="Index">Back to List</a>
</div>



